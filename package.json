{
  "name": "@garbee/material-icons",
  "version": "0.0.0",
  "description": "Material icons in SVG form for JavaScript projects",
  "files": [
    "dist",
    "svg"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./icons/filled": {
      "types": "./dist/icons/filled/icons.d.ts",
      "default": "./dist/icons/filled/icons.js"
    },
    "./icons/outlined": {
      "types": "./dist/icons/outlined/icons.d.ts",
      "default": "./dist/icons/outlined/icons.js"
    },
    "./icons/round": {
      "types": "./dist/icons/round/icons.d.ts",
      "default": "./dist/icons/round/icons.js"
    },
    "./icons/sharp": {
      "types": "./dist/icons/sharp/icons.d.ts",
      "default": "./dist/icons/sharp/icons.js"
    },
    "./icons/two-tone": {
      "types": "./dist/icons/two-tone/icons.d.ts",
      "default": "./dist/icons/two-tone/icons.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "del icons svg dist",
    "gather-icons": "node bin/gather-icons.mjs",
    "convert-icons": "svg-to-ts-files --config etc/svg-to-ts/icons.js",
    "icons": "run-s clean gather-icons convert-icons build"
  },
  "author": "Jonathan Garbee <jonathan@garbee.me>",
  "license": "Apache-2.0",
  "devDependencies": {
    "del-cli": "^5.0.0",
    "glob": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "svg-to-ts": "^10.0.0",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Garbee/material-icons.git"
  }
}
