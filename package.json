{
  "name": "@garbee/material-icons",
  "version": "0.0.0",
  "description": "Material icons in SVG form for JavaScript projects",
  "files": [
    "icons",
    "svg",
    "index.js",
  ],
  "main": "./index.js",
  "module": "./index.js",
  "exports": {
    ".": {
      "default": "./index.js"
    },
    "./icons/filled": {
      "default": "./icons/filled/icons.js"
    },
    "./icons/outlined": {
      "default": "./icons/outlined/icons.js"
    },
    "./icons/round": {
      "default": "./icons/round/icons.js"
    },
    "./icons/sharp": {
      "default": "./icons/sharp/icons.js"
    },
    "./icons/two-tone": {
      "default": "./icons/two-tone/icons.js"
    }
  },
  "type": "module",
  "scripts": {
    "make": "run-s clean gather-icons set:common convert-icons set:module",
    "clean": "del icons svg",
    "gather-icons": "node bin/gather-icons.mjs",
    "convert-icons": "svg-to-ts-files --config etc/svg-to-ts/icons.js",
    "icons": "run-s clean gather-icons convert-icons build",
    "set:common": "npm pkg set 'type=commonjs'",
    "set:module": "npm pkg set 'type=module'"
  },
  "author": "Jonathan Garbee <jonathan@garbee.me>",
  "license": "Apache-2.0",
  "devDependencies": {
    "del-cli": "^5.0.0",
    "glob": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "svg-to-ts": "^10.0.0",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Garbee/material-icons.git"
  }
}
